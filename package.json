{
  "name": "tire-backend",
  "version": "1.0.0",
  "description": "Backend server for tire-demo",
  "scripts": {
    "pull_dbschema": "npx prisma db pull && npx prisma generate",
    "clean": "npx tsc --build --clean",
    "prebuild": "eslint -c .eslintrc.cjs",
    "test": "dotenv -e .env.development -- npx jest --silent false --verbose true --collectCoverage",
    "build": "npx tsc -p tsconfig.build.json",
    "reg_start": "cross-env NODE_ENV=production node --import ./dist/register.js ./dist/index.js",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "wp_build": "cross-env NODE_ENV=production npx webpack --fail-on-warnings --config webpack.config.cjs",
    "wp_start": "node ./build/bundle.cjs",
    "postinstall": "npx prisma generate",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts"
  },
  "type": "module",
  "keywords": [],
  "author": "Hyman",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/jsonwebtoken": "^9.0.5",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan-body": "^2.6.9",
    "nanoid": "^5.0.4",
    "resolve": "^1.22.8",
    "response-time": "^2.3.2",
    "save": "^2.9.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/response-time": "^2.3.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  }
}
