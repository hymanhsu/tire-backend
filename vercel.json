{
    "name": "tire-backend",
    "version": 2,
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },
        {
            "src": "**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}